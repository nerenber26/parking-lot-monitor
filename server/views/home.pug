doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" href="/stylesheets/default.css")
    title Home
  body
    header
      .topnav 
        a.active(href='/') Home
        a(href='/map') Map
        a(href='/query') Query
    .myspacer
      each lot in results
        .info
          p #{lot.name}: #{lot.available_spaces}/#{lot.total_spaces} !{lot.icon}
        .myspacer